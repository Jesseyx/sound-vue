{
  "env": {
    "browser": true
  },
  "root": true,
  "extends": "airbnb-base",
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module"
  },
  "plugins": [
    "html"
  ],
  "rules": {
    "import/prefer-default-export": 0,
    "no-use-before-define": 0,
    "no-param-reassign": ["error", { "props": false }],
    "no-console": 0
  }
}
